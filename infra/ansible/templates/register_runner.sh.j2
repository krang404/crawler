#!/bin/bash

docker exec -it gitlab-gitlab-runner-1 gitlab-runner register  \
--url http://{{ YOUR_VM_IP }}/ \
--non-interactive \
--locked=false \
--name DockerRunner \
--executor docker \
--docker-image docker:stable \
--docker-privileged \
--env YA_REGISTRY={{ YA_REGISTRY }} \
--registration-token {{ YOUR_TOKEN }} \
--tag-list "linux,xenial,ubuntu,docker" \
--run-untagged
